{
  "name": "datascribe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && tsc",
    "start:prod": "node build/app.js",
    "start": "nodemon src/app.ts",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "seeder": "nodemon ./src/common/loaders/seeder.ts $1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "tsc && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "lint-staged": "lint-staged",
    "dev": "NODE_ENV=development nodemon --ignore documents --ignore vector-cache --ignore storage --trace-warnings index.js",
    "db:migrate:create": "npx sequelize-cli migration:create --name",
    "db:seed:create": "npx sequelize-cli seed:generate --name",
    "db:migrate:run": "npx sequelize-cli db:migrate --env development",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all --env development",
    "db:seed:run": "npx sequelize-cli db:seed:all --env development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@googleapis/youtube": "^9.0.0",
    "@pinecone-database/pinecone": "^0.1.6",
    "body-parser": "^1.20.2",
    "celebrate": "^14.0.0",
    "chromadb": "^1.5.2",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jm-ez-l10n": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "langchain": "^0.0.90",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^3.2.1",
    "pg": "^8.7.3",
    "pinecone-client": "^1.1.0",
    "sequelize": "^6.21.0",
    "slugify": "^1.6.6",
    "sqlite": "^4.2.1",
    "sqlite3": "^5.1.6",
    "typedi": "^0.10.0",
    "uuid": "^9.0.0",
    "vectordb": "0.1.5",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "ejs": "^3.1.9",
    "helmet": "^7.0.0",
    "nodemailer": "^6.9.3",
    "socket.io": "^4.7.0"
  },
  "devDependencies": {
    "@types/sequelize": "^4.28.13",
    "nodemon": "^2.0.22",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.5.1",
    "ts-node": "^10.8.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "@types/express": "^4.17.17"
  }
}
