{
  "name": "bstamp",
  "version": "0.0.1",
  "description": "bStamp API",
  "main": "src/app.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc &&  mkdir ./build/common/language &&  cp ./src/common/language/translation.en.json ./build/common/language/translation.en.json && mkdir ./build/common/views && cp -R ./src/common/views/* ./build/common/views && cp ./private.pem ./build/private.pem && cp ./src/common/loaders/sandboxWorker.ts ./build/common/loaders/sandboxWorker.ts && cp ./src/global.ts ./build/global.ts",
    "start": "nodemon",
    "start:prod": "node build/app.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "seeder": "nodemon ./src/common/loaders/seeder.ts $1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "tsc && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devEdexa/bStamp-api.git"
  },
  "keywords": [
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "edeXa",
  "license": "ISC",
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.75",
    "aws-sdk": "^2.1178.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "bullmq": "^1.91.1",
    "celebrate": "^13.0.4",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "events": "^3.3.0",
    "express": "^4.18.1",
    "jm-ez-l10n": "^1.0.0",
    "joi-objectid": "^4.0.2",
    "jsbarcode": "^3.11.5",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.1.0",
    "moment": "^2.29.4",
    "mongoose": "^6.4.5",
